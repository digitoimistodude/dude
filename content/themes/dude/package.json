{
  "name": "dude",
  "version": "1.0.0",
  "description": "dude powered by dudestack + devpackages",
  "author": "Digitoimisto Dude Oy (moro@dude.fi)",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@digitoimistodude/code-quality-checks": "^2.1.0",
    "@digitoimistodude/stylelint-config": "^1.0.0",
    "@hint/hint-axe": "^4.4.13",
    "@hint/hint-button-type": "^3.0.15",
    "@hint/hint-compat-api": "^4.4.12",
    "@parcel/transformer-sass": "^2.16.3",
    "@swup/body-class-plugin": "^3.3.0",
    "@swup/scripts-plugin": "^2.1.0",
    "@swup/scroll-plugin": "^4.0.0",
    "@vimeo/player": "^2.30.1",
    "@wordpress/scripts": "^31.2.0",
    "autoprefixer": "^10.4.23",
    "browser-sync": "^2.27.10",
    "concurrently": "^9.2.1",
    "cssnano": "^7.1.2",
    "cssnano-preset-advanced": "^7.0.10",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "hamburgers": "^1.2.1",
    "hint": "^7.0.1",
    "husky": "^9.1.7",
    "moveto": "^1.8.2",
    "npm-check-updates": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.16.3",
    "postcss": "^8.5.6",
    "postcss-calc": "^8.2.4",
    "prettier": "^3.6.2",
    "reframe.js": "^4.0.2",
    "sanitize.css": "^13.0.0",
    "sass": "^1.97.2",
    "slick-carousel": "^1.8.1",
    "stylelint": "^15.11.0",
    "swiper": "^8.3.2",
    "swup": "^4.8.2",
    "what-input": "^5.2.12"
  },
  "scripts": {
    "dev": "concurrently --kill-others -n \"CSS,JS,BLOCKS,SYNC\" \"npm run watch:styles\" \"npm run watch:js\" \"npm run watch:blocks\" \"npm run browsersync\"",
    "browsersync": "node scripts/browsersync.js",
    "build": "npm-run-all build:styles build:js build:blocks",
    "build:styles": "NODE_ENV=production parcel build assets/src/sass/global.scss assets/src/sass/gutenberg-editor-styles.scss assets/src/sass/ama.scss assets/src/sass/surveys.scss --dist-dir assets/dist/css --no-cache",
    "build:js": "NODE_ENV=production parcel build assets/src/js/front-end.js assets/src/js/gutenberg-editor.js assets/src/js/legacy.js assets/src/js/video-player.js --dist-dir assets/dist/js --no-cache",
    "build:blocks": "npm-run-all --parallel build:block-*",
    "build:block-hero": "wp-scripts build blocks/pricing-hero/src/index.js --output-path=blocks/pricing-hero/build",
    "build:block-category": "wp-scripts build blocks/pricing-category/src/index.js --output-path=blocks/pricing-category/build",
    "build:block-item": "wp-scripts build blocks/pricing-item/src/index.js --output-path=blocks/pricing-item/build",
    "build:block-cta": "wp-scripts build blocks/pricing-cta/src/index.js --output-path=blocks/pricing-cta/build",
    "build:block-faq": "wp-scripts build blocks/pricing-faq/src/index.js --output-path=blocks/pricing-faq/build",
    "watch:styles": "PARCEL_CONFIG=.parcelrc.watch parcel watch assets/src/sass/global.scss assets/src/sass/gutenberg-editor-styles.scss assets/src/sass/ama.scss assets/src/sass/surveys.scss --dist-dir assets/dist/css --no-hmr --no-cache --no-source-maps",
    "watch:js": "PARCEL_CONFIG=.parcelrc.watch parcel watch assets/src/js/front-end.js assets/src/js/gutenberg-editor.js assets/src/js/legacy.js assets/src/js/video-player.js --dist-dir assets/dist/js --no-hmr --no-cache --no-source-maps",
    "watch:blocks": "npm-run-all --parallel watch:block-*",
    "watch:block-hero": "wp-scripts start blocks/pricing-hero/src/index.js --output-path=blocks/pricing-hero/build",
    "watch:block-category": "wp-scripts start blocks/pricing-category/src/index.js --output-path=blocks/pricing-category/build",
    "watch:block-item": "wp-scripts start blocks/pricing-item/src/index.js --output-path=blocks/pricing-item/build",
    "watch:block-cta": "wp-scripts start blocks/pricing-cta/src/index.js --output-path=blocks/pricing-cta/build",
    "watch:block-faq": "wp-scripts start blocks/pricing-faq/src/index.js --output-path=blocks/pricing-faq/build",
    "lint:styles": "stylelint 'assets/src/sass/**/*.scss'",
    "lint:js": "eslint 'assets/src/js/**/*.js' 'blocks/*/src/**/*.js'",
    "lint:php": "phpcs --standard=../../../phpcs.xml *.php inc/**/*.php template-parts/**/*.php",
    "test": "npm run lint:styles && npm run lint:js && npm run lint:php && npm run format:check",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,php}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,php}\"",
    "prepare": "cd ../../.. && husky"
  },
  "dependencies": {
    "airbnb-browser-shims": "^3.3.0",
    "canvas-confetti": "^1.9.4",
    "npx": "^10.2.2"
  },
  "alias": {
    "swiper/core": "swiper"
  }
}
