{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "dude/pricing-hero",
  "version": "1.0.0",
  "title": "Hero",
  "category": "dude-native",
  "icon": "money-alt",
  "description": "Hinnaston hero-osio otsikolla ja ingressillä",
  "keywords": ["pricing", "hero", "hinnasto"],
  "attributes": {
    "paddingTopDesktop": {
      "type": "number",
      "default": 64
    },
    "paddingBottomDesktop": {
      "type": "number",
      "default": 0
    },
    "paddingTopMobile": {
      "type": "number",
      "default": 75
    },
    "paddingBottomMobile": {
      "type": "number",
      "default": 0
    }
  },
  "supports": {
    "html": false,
    "align": ["full"],
    "anchor": true
  },
  "example": {
    "innerBlocks": [
      {
        "name": "core/heading",
        "attributes": {
          "level": 1,
          "content": "Hinnasto"
        }
      },
      {
        "name": "core/heading",
        "attributes": {
          "level": 2,
          "content": "Investoi parempaan digitaaliseen läsnäoloon"
        }
      },
      {
        "name": "core/paragraph",
        "attributes": {
          "content": "Kaikki palvelumme ovat räätälöityjä ja suunniteltu kestämään aikaa."
        }
      }
    ]
  },
  "textdomain": "dude",
  "editorScript": "file:./build/index.js",
  "style": "file:./build/style-index.css"
}
